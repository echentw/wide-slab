{
  "name": "Wide Slab",
  "description": "Allow content in slab.com to be wider",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://*.slab.com/*"],
      "js": ["slab.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
